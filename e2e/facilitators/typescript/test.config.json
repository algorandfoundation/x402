{
  "name": "typescript",
  "type": "facilitator",
  "language": "typescript",
  "protocolFamilies": [
    "avm",
    "evm",
    "svm",
    "aptos"
  ],
  "x402Versions": [
    1,
    2
  ],
  "extensions": [
    "bazaar",
    "eip2612GasSponsoring"
  ],
  "evm": {
    "transferMethods": ["eip3009", "permit2"]
  },
  "environment": {
    "required": [
      "PORT",
      "EVM_PRIVATE_KEY",
      "SVM_PRIVATE_KEY"
    ],
    "optional": [
      "AVM_PRIVATE_KEY",
      "APTOS_PRIVATE_KEY",
      "AVM_NETWORK",
      "EVM_NETWORK",
      "SVM_NETWORK",
      "APTOS_NETWORK",
      "ALGOD_SERVER",
      "ALGOD_TOKEN"
    ]
  }
}
